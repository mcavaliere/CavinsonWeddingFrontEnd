    "react": "^15.4.2",
    "react-bootstrap": "^0.30.8",
    "react-burger-menu": "^1.10.14",
    "react-dom": "^15.4.2",
    "react-ga": "^2.1.2",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.2",
    "react-router-bootstrap": "^0.23.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "scrollmagic": "^2.0.5",
    "serve-favicon": "^2.4.0",
    "smoothscroll": "^0.3.0",
    "style-loader": "^0.13.2"
