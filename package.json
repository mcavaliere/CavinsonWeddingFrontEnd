{
  "name": "cavinson_wedding_front_end",
  "version": "1.0.0",
  "description": "Front-end for our wedding website!",
  "main": "index.js",
  "scripts": {
    "test": "test",
    "prestart": "babel-node tools/startMessage.js",
    "start": "npm-run-all --parallel open:src lint:watch",
    "open:src": "babel-node tools/srcServer.js",
    "lint": "node_modules/.bin/esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "clean-public": "npm run remove-public && mkdir public",
    "remove-public": "node_modules/.bin/rimraf ./public",
    "build:html": "babel-node tools/buildHtml.js",
    "build:server": "babel tools/publicServer.js --out-file tools/publicServer-compiled.js",
    "build:js": "babel-node tools/build.js",
    "build": "npm-run-all clean-public build:server build:html build:js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mcavaliere/CavinsonWeddingFrontEnd.git"
  },
  "author": "Mike Cavaliere",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/mcavaliere/CavinsonWeddingFrontEnd/issues"
  },
  "homepage": "https://github.com/mcavaliere/CavinsonWeddingFrontEnd#readme",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-register": "^6.23.0",
    "bootstrap-sass": "^3.3.7",
    "bourbon": "^4.3.3",
    "cheerio": "^0.22.0",
    "colors": "^1.1.2",
    "css-loader": "^0.26.2",
    "eslint": "^3.16.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.10.0",
    "eslint-watch": "^3.0.0",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.10.1",
    "image-webpack-loader": "^3.3.0",
    "imports-loader": "^0.7.1",
    "napa": "^2.3.0",
    "node-sass": "^4.5.0",
    "npm-run-all": "^4.0.2",
    "sass-loader": "^6.0.2",
    "style-loader": "^0.13.2",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.17.1"
  },
  "dependencies": {
    "auth0-lock": "^10.11.0",
    "bootstrap-sass": "^3.3.7",
    "colors": "^1.1.2",
